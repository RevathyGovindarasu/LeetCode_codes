class Solution {
    public int partitionDisjoint(int[] nums) {
       int max1 = nums[0], index=0, max2 = max1;
        for(int i=1;i<nums.length;i++)
        {
            if(max1>nums[i])
            {
                max1=max2;
                index=i;
            }
            else
                max2=Math.max(max2,nums[i]);
        }
            return index+1;
        
    }
}
